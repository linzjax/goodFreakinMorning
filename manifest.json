{
	"manifest_version":2,

	"name": "Good Freakin Morning",
	"description": "This extension turns your front page into a freakin motivational buddy to make sure you're gettin stuff done!",
	"version":"1.0",

	"permissions":[
		"tabs", 
		"storage",
	],
	"chrome_url_overrides" : {
		"newtab" : "goodFreakinMorning.html"
	},
	"content_scripts":[
	{
		"matches": ["http://www.google.com/*"],
		"css": ["main.css"],
		"js": ["jquery.js", "trello.js"]
	}],
	"content_security_policy": "script-src 'self' https://api.trello.com/*; object-src 'self'",
	
}